import java.util.Scanner; // 1900 ile 2100 yılları arasındaki Asal Yılları bulalım.

public class Main {
	
	public static boolean AsalSayi(int sayi) {
		
		for(int i=2;i<sayi;i++){
			
			if(sayi%i==0) {
				
				return false;
			}
		}
		return true;	
	}

	public static void main(String[] args) {
		
		for(int i=1900; i<2100;i++ ) {
			
			if(AsalSayi(i)) {
				
				System.out.println(i);
				
			}
		}
		
        }
  }
